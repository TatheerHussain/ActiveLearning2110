extends default


block content
    .div(ng-controller='dashController as dash')
        .userID(value=id)
        +navmenu('main-nav', 'left', 'custom')
            .profile-container.clearfix
                +img-circle-left(photo)
                .contrainer
                    p.uppercase.voffset3 #{username}
                    p.capitalize.rounded-back #{role}
            .profile-sep.voffset3
            p.uppercase.voffset3 course list
            p.voffset2(ng-repeat='course in dash.courses') {{course}}
    +navbar-menu('Active Learning 2110', 'main-nav')
    .container.dash
        h1.text-center #{id}

block extraScripts
    script(src='./scripts/dashboard.js')
